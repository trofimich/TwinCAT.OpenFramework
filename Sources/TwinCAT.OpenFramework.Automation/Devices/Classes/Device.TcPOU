    <Method Name="Disable" Id="{456e8034-80c7-4023-bbc1-b1fa7ea41430}">
      <Declaration><![CDATA[METHOD FINAL Disable]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT _Enabled THEN
	RETURN;
END_IF

OnEnabledChange(FALSE);

_Enabled := FALSE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="Enable" Id="{da30d30f-9904-4df0-aa77-06867eb85fe2}">
      <Declaration><![CDATA[METHOD FINAL Enable]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Enabled THEN
	RETURN;
END_IF

OnEnabledChange(TRUE);

_Enabled := TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="Initialize" Id="{b3b19006-a5ea-4fbb-ad1a-6167b8192dbd}">
      <Declaration><![CDATA[METHOD FINAL Initialize : BOOL
VAR
	memberNotSpecifiedException : DataMemberNotSpecifiedException;	
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _Parent = 0 THEN
	memberNotSpecifiedException.Throw(Namespacename, ClassName, __POUNAME(), __POSITION(), 'Parent');
END_IF

Initialize := OnInitialize();]]></ST>
      </Implementation>
    </Method>
