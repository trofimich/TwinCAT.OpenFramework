    <Method Name="SetSimulationModeOverride" Id="{549cfc53-c096-44f3-b84a-de18239875ca}">
      <Declaration><![CDATA[METHOD FINAL SetSimulationModeOverride
VAR_INPUT
	value : SIMULATION_MODE_OVERRIDE;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _SimulationModeOverride = value THEN
	RETURN;
END_IF

OnSimulationModeOverrideChange(value);

_SimulationModeOverride := value;]]></ST>
