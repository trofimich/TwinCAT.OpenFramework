{attribute 'no_explicit_call' := 'do not call this function block directly'} 
FUNCTION_BLOCK ABSTRACT Condition EXTENDS Core.Object IMPLEMENTS ICondition
VAR
	{attribute 'OPC.UA.DA' := '0'}
	_Name : WSTRING;

	{attribute 'OPC.UA.DA' := '0'}
	_Workflow : IWorkflow;

	{attribute 'OPC.UA.DA' := '0'}
	_ParentActivity : IActivity;
	
	{attribute 'OPC.UA.DA' := '0'}
	_ParentCondition : ICondition;

	{attribute 'OPC.UA.DA' := '0'}
	_Variables : Collections.Dictionary(0, 0);
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Folder Name="Methods to override" Id="{5ed654de-488c-41a3-9dd6-d31befe37354}" />
    <Property Name="Configured" Id="{167f4971-9594-4ce6-95be-09c7e6a87131}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ABSTRACT Configured : BOOL]]></Declaration>
      <Get Name="Get" Id="{d14f9b9f-000e-4358-a872-00cc7a327f4d}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="Evaluate" Id="{79c4792c-02ed-4587-8f2d-2a9ccd61ae06}">
      <Declaration><![CDATA[METHOD FINAL Evaluate : BOOL
VAR_INPUT
	activity : IActivity;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF OnEvalute(activity) THEN
	Evaluate := TRUE;
	
	LogManager.TryLogMessage(WideStringHelper.ConcatStrings255("Condition '", Name, "' evaluated as TRUE at ", TO_WSTRING(SystemDateTimeManager.SystemLocalDateTime)), LOG_CATEGORY.SUCCESS, TO_WSTRING(ClassName), "Condition", "ConditionEvaluate");
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{4bec2862-106d-4344-bbd9-4598c29c12e2}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	(* if TRUE, the retain variables are initialized (warm start / cold start)*)
	bInitRetains	: BOOL;
	(* if TRUE, the instance afterwards gets moved into the copy code (online change)*)
	bInCopyCode	: BOOL;
	workflow : IWorkflow;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[_Workflow := workflow;

IF _Workflow <> 0 THEN
	_Workflow.RegisterChild(THIS^);
END_IF]]></ST>
      </Implementation>
    </Method>
    <Property Name="Name" Id="{71961bbf-f4ff-4ba9-a45a-cd1279b7b5ac}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY FINAL Name : WSTRING
]]></Declaration>
      <Get Name="Get" Id="{f1799a45-8028-470b-acc1-25f2f24a0e72}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[Name := SEL(_Name = "", _Name, TO_WSTRING(ClassName));]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{df79a823-5566-422a-bdb9-d7bb776fc316}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[_Name := Name;]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Method Name="OnEvalute" Id="{679374e5-c40e-49a4-b386-c18b7d32a90f}" FolderPath="Methods to override\">
      <Declaration><![CDATA[METHOD PROTECTED ABSTRACT OnEvalute : BOOL
VAR_INPUT
	activity : IActivity;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[]]></ST>
      </Implementation>
    </Method>
    <Property Name="Parent" Id="{40367a00-bd78-4469-98b1-577fa9c041fd}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY FINAL Parent : IWorkflowElement]]></Declaration>
      <Get Name="Get" Id="{0f9d4991-246e-4d36-9ce8-fbe0351b39ca}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[IF _ParentCondition <> 0 THEN
	Parent := _ParentCondition;
ELSE
	Parent := _ParentActivity;
END_IF]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="ParentActivity" Id="{b6256e22-b8a0-4cc3-a832-607b87034c7f}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY FINAL ParentActivity : IActivity]]></Declaration>
      <Get Name="Get" Id="{51d2327b-3bc3-49d3-8be6-ff68ae13904b}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ParentActivity := _ParentActivity;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{41b8ec15-87fc-4bba-8ef5-a5827c202fef}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[_ParentActivity := ParentActivity;
_ParentCondition := 0;]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Property Name="ParentCondition" Id="{c33d6432-48ad-41c4-919b-becbef5c626f}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY FINAL ParentCondition : ICondition]]></Declaration>
      <Get Name="Get" Id="{f889bd28-4875-415c-9f55-b2b9cfb6479e}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ParentCondition := _ParentCondition;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{dc850025-6a74-45a0-9116-b83341f47346}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[_ParentCondition := ParentCondition;

IF ParentCondition <> 0 THEN
	_ParentActivity := ParentCondition.ParentActivity;
END_IF]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Method Name="TryGetVariable" Id="{54c0b438-a23e-4e9d-a1fa-12698a06e3ed}">
      <Declaration><![CDATA[METHOD TryGetVariable : BOOL
VAR_INPUT
	name : STRING;
END_VAR
VAR_OUTPUT
	variable : Core.GENERIC_VALUE;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF TryGetVariable := _Variables.TryGetValueByKey(name, value => variable) THEN
	RETURN;
END_IF

IF _ParentCondition <> 0 AND_THEN (TryGetVariable := _ParentCondition.TryGetVariable(name, variable => variable)) THEN
	RETURN;
END_IF

IF _ParentActivity <> 0 AND_THEN (TryGetVariable := _ParentActivity.TryGetVariable(name, variable => variable)) THEN
	RETURN;
END_IF

IF _Workflow <> 0 THEN
	TryGetVariable := _Workflow.TryGetVariable(name, variable => variable);
END_IF]]></ST>
      </Implementation>
    </Method>
  </POU>
</TcPlcObject>