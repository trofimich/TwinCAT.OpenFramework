    <Method Name="Evaluate" Id="{79c4792c-02ed-4587-8f2d-2a9ccd61ae06}">
      <Declaration><![CDATA[METHOD FINAL Evaluate : BOOL
VAR_INPUT
	workflow : IWorkflow;
	activity : IActivity;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF OnEvalute(workflow, activity) THEN
	Evaluate := TRUE;
	
	LogManager.TryLogMessage(WideStringHelper.ConcatStrings255("Condition '", Name, "' evaluated as TRUE at ", TO_WSTRING(SystemDateTimeManager.SystemLocalDateTime)), LOG_CATEGORY.SUCCESS, TO_WSTRING(ClassName), "Condition", "Evaluate");
END_IF]]></ST>
