{attribute 'enable_dynamic_creation'}	
FUNCTION_BLOCK DemoWorkflow EXTENDS Workflow<6>
VAR_INPUT
	XPosition : INT;
	YPosition : INT;
	
	MoveFrom1To2 : BOOL;
	MoveFrom2To3 : BOOL;	
	MoveFrom3To4 : BOOL;	
	MoveFrom4To1 : BOOL;
END_VAR
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{b5279f96-3cc0-4b10-94f1-c41eea8241ef}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{d0b1f1e5-44e8-44bf-afb9-34b1f18937f7}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="CreateVariables" Id="{51b26f57-2957-4301-a216-0f399dc62fe6}">
      <Declaration><![CDATA[METHOD PROTECTED CreateVariables : ARRAY [0..VariableCount - 1] OF VARIABLE_DESCRIPTOR]]></Declaration>
      <Implementation>
        <ST><![CDATA[CreateVariables[0].Initialize('XPosition', XPosition);
CreateVariables[1].Initialize('YPosition', YPosition);

CreateVariables[2].Initialize('MoveFrom1To2', MoveFrom1To2);
CreateVariables[3].Initialize('MoveFrom2To3', MoveFrom2To3);
CreateVariables[4].Initialize('MoveFrom3To4', MoveFrom3To4);
CreateVariables[5].Initialize('MoveFrom4To1', MoveFrom4To1);]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{2ac10e27-5c07-430f-8815-6a49562c7896}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	(* if TRUE, the retain variables are initialized (warm start / cold start)*)
	bInitRetains	: BOOL;
	(* if TRUE, the instance afterwards gets moved into the copy code (online change)*)
	bInCopyCode	: BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[_Name := "Demo workflow";]]></ST>
      </Implementation>
    </Method>
    <Property Name="Size" Id="{5b4d1388-29da-418f-aee0-1a2a0c82dc24}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY Size : ULINT]]></Declaration>
      <Get Name="Get" Id="{bebd553c-a106-43a4-afc0-d3e65361e96a}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[Size := XSIZEOF(THIS^);]]></ST>
        </Implementation>
      </Get>
    </Property>
  </POU>
</TcPlcObject>