    <Method Name="Clear" Id="{0def1be0-078f-4e42-b9d9-bacd7c63976c}">
      <Declaration><![CDATA[METHOD Clear : IUniqueSet
VAR
	i : DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[Clear := THIS^;

FOR i := 0 TO _ValueCount - 1 DO
	_Array[i].Release();						
END_FOR	

_ValueCount := 0;
_ArrayLength := 0;

IF _Array <> 0 THEN
	__DELETE(_Array);	
END_IF]]></ST>
      </Implementation>
    </Method>
