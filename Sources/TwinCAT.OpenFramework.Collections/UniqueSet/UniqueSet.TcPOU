    <Method Name="RemoveAt" Id="{560da408-7178-47ba-9120-b01c5ed8fd70}">
      <Declaration><![CDATA[METHOD RemoveAt : IUniqueSet
VAR_INPUT
	index : DINT;
END_VAR
VAR
	argumentOutOfRangeException : ArgumentOutOfRangeException;

	i : DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF index < 0 OR_ELSE index >= _ValueCount THEN
	argumentOutOfRangeException.Throw(NamespaceName, ClassName, __POUNAME(), __POSITION(), 'index', index);
END_IF

RemoveAt := THIS^;

_Array[index].Release();

IF index + 2 <= _ValueCount THEN
	FOR	i := index TO _ValueCount - 2 DO
		_Array[i] := _Array[i + 1];
	END_FOR			
END_IF
	
_Array[_ValueCount - 1].Reset();

_ValueCount := _ValueCount - 1;]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetCopy" Id="{942d6884-b40d-46d4-872b-4443fcd5acea}">
      <Declaration><![CDATA[METHOD SetCopy : IUniqueSet
VAR_INPUT
	value : ANY;
	isObject : BOOL := FALSE;
END_VAR
VAR_OUTPUT
	index : DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[SetCopy := SetGeneric(GenericValueFactory.FromAnyType(value, isObject, GENERIC_MEMORY_MANAGEMENT_BEHAVIOR.CLONE_ORIGINAL_VALUE_AND_OWNS_MEMORY), index => index);]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetGeneric" Id="{e49ee7f0-ea9d-45ea-8ca1-9435721d8384}">
      <Declaration><![CDATA[METHOD SetGeneric : IUniqueSet
VAR_INPUT
	value : GENERIC_VALUE;
END_VAR
VAR_OUTPUT
	index : DINT;
END_VAR
VAR
	supposedIndex, i : DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _ValueValidator <> 0 THEN
	_ValueValidator.Validate(Name, value.TypeClass);
END_IF

SetGeneric := THIS^;

// if equal key found then we need to replace it
IF FindIndex(value.AsAnyType(), supposedIndex => supposedIndex) THEN	
	_Array[supposedIndex].Release(); 
ELSE
	IF _ValueCount >= _ArrayLength THEN
		AllocateMemory(DefaultAllocationElemetCount);
	END_IF
		
	FOR i := _ValueCount - 1 TO supposedIndex BY -1 DO	
		_Array[i + 1] := _Array[i];
	END_FOR

	_ValueCount := _ValueCount + 1;
END_IF

_Array[supposedIndex] := value;

index := supposedIndex;]]></ST>
      </Implementation>
    </Method>
