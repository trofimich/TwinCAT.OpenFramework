      </Get>
    </Property>
    <Property Name="SelfSize" Id="{c7958ef8-cf29-412d-b3ad-0101df0c5c1a}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY SelfSize : ULINT]]></Declaration>
      <Get Name="Get" Id="{7b643129-d919-4326-bde2-8d1483b19251}">
