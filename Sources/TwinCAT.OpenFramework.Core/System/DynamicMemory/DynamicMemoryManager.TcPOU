<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="DynamicMemoryManager" Id="{dfd03ea7-11b7-42ce-b52e-f5a7df7a7ad3}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM DynamicMemoryManager
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{c33db740-6bfc-43f8-9d4d-602279175d35}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{fc745387-f67a-4d86-bdee-e470addbc3c6}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="ReleaseDynamicMemory" Id="{5fe5bc2a-b608-418e-9ff0-e9c1a91c92f5}">
      <Declaration><![CDATA[METHOD ReleaseDynamicMemory
VAR_INPUT
	value : POINTER TO BYTE;
	isObject : BOOL;
END_VAR
VAR
	memoryArea : Tc2_System.E_TcMemoryArea;
	objectPointer : POINTER TO Object;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF value = 0 THEN
	RETURN;
END_IF

// check at list first byte of released memory
memoryArea := Tc2_System.F_CheckMemoryArea(value, 1);

IF memoryArea <> E_TcMemoryArea.Dynamic THEN
	RETURN;
END_IF

IF isObject THEN
	objectPointer := value;
	// to call FB_exit
	__DELETE(objectPointer);
ELSE
	__DELETE(value);
END_IF]]></ST>
      </Implementation>
    </Method>
  </POU>
</TcPlcObject>