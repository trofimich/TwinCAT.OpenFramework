    <Method Name="AppendToTarget" Id="{7d3432ad-7058-4ce7-80d2-01c9d35ec6fb}" FolderPath="Append\">
      <Declaration><![CDATA[METHOD AppendToTarget
VAR_INPUT
	targetString : ANY_STRING;
END_VAR
VAR_IN_OUT CONSTANT
