    <Method Name="ReplaceInSource" Id="{f06898de-32ea-4f51-b053-1b282cb73a8a}">
      <Declaration><![CDATA[METHOD ReplaceInSource
VAR_INPUT
	sourceString : ANY_STRING;
END_VAR
VAR_IN_OUT CONSTANT
	oldValue : STRING;
	newValue : STRING;
END_VAR
VAR_INPUT
	maxReplacementCount : UINT := 0;
	skip : UINT := 0;
END_VAR
VAR
	stringTypeNotSupportedException : NotSupportedException;
	sourceStringMemoryOverflowExcepton : TargetVariableMemoryOwerflowException;

	sourceStringMemorySize : UDINT;
	sourceStringLength : UDINT;
	
	oldValueLength : UDINT;
	newValueLength : UDINT;
	checkMemorySize : BOOL;	
	
	i : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF sourceString.TypeClass <> __SYSTEM.TYPE_CLASS.TYPE_STRING THEN
	stringTypeNotSupportedException.Throw(NamespaceName, ClassName, __POUNAME(), __POSITION(), Tc2_Utilities.WCONCAT("Not supported type ", TO_WSTRING(sourceString.TypeClass)), TRUE);
END_IF

sourceStringMemorySize := TO_UDINT(sourceString.diSize);
sourceStringLength := Tc2_Utilities.LEN2(sourceString.pValue);

oldValueLength := Tc2_Utilities.LEN2(ADR(oldValue));
newValueLength := Tc2_Utilities.LEN2(ADR(newValue));
checkMemorySize := oldValuelength < newValueLength;

WHILE 1 = 1 DO
	i := Tc2_Utilities.FIND2(sourceString.pValue + i, ADR(oldValue));
	
	IF i = 0 THEN
		EXIT;
	END_IF
	
	//IF checkMemorySize AND_THEN sourceStringLength + newValueLength - oldValueLength + 1 > sourceStringMemorySize THEN
		
	//END_IF
END_WHILE

(*IF NOT InternalTryAppendToTarget(targetString, sourceString1, sourceString2, sourceString3, sourceString4, sourceString5, sourceString6, sourceString7, sourceString8, sourceString9, sourceString10) THEN
	sourceStringMemoryOverflowExcepton.Throw(NamespaceName, ClassName, __POUNAME(), __POSITION(), 'targetString', TO_ULINT(targetString.diSize)); 
END_IF*)]]></ST>
      </Implementation>
    </Method>
