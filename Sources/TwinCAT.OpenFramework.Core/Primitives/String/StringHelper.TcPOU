	stringTypeNotSupportedException.Throw(CurrentNamespace.Name, ClassName, __POUNAME(), __POSITION(), Tc2_Utilities.WCONCAT("Not supported type ", TO_WSTRING(destinationString.TypeClass)), TRUE);
