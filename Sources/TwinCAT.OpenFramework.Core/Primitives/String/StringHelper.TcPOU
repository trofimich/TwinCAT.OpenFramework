    <Method Name="DateAndTimeToString" Id="{28fcad9c-7fcb-4186-a1d5-d74d8064f18b}">
      <Declaration><![CDATA[METHOD DateAndTimeToString : STRING
VAR_INPUT
	value : DATE_AND_TIME;
	dateValuesSeparator : STRING(1) := '_';
	dateAndTimeSeparator : STRING(1) := '-';
