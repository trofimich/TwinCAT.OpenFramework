    <Method Name="AppendString255" Id="{35adae7e-a852-4be2-b34c-8fd47200dbd7}">
      <Declaration><![CDATA[METHOD AppendString255 : BOOL
VAR_INPUT
	destinationString : ANY_STRING;
	sourceString1 : STRING(255);
