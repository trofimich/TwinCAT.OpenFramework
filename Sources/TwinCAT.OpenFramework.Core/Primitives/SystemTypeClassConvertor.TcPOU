    DATA_TYPE.TYPE_OBJECT:
		GetSystemTypeClassByDataType := __SYSTEM.TYPE_CLASS.TYPE_USERDEF;

	ELSE
		dataTypeNotSupportedException.Throw(CurrentNamespace.Name, ClassName, __POUNAME(), __POSITION(), 'dataType', dataType);
END_CASE]]></ST>
      </Implementation>
