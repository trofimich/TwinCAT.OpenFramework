    <Method Name="GetSystemTypeClassByDataType" Id="{d9c3bb0b-6fa5-4b7c-8032-a6a81d60a893}">
      <Declaration><![CDATA[METHOD GetSystemTypeClassByDataType : __SYSTEM.TYPE_CLASS
