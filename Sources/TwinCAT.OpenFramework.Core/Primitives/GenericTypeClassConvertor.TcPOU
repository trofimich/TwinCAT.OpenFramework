          <ST><![CDATA[NamespaceName := CurrentNamespace.Name;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="TryGetGenericTypeClassBySystemAnyType" Id="{55a1f39a-2f1b-46da-b162-f7d95a55b732}">
      <Declaration><![CDATA[METHOD TryGetGenericTypeClassBySystemAnyType : BOOL
VAR_INPUT
	systemAnyType : __SYSTEM.AnyType;	
	isObject : BOOL := FALSE;
END_VAR
VAR_OUTPUT
	genericTypeClass : GENERIC_TYPE_CLASS;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[TryGetGenericTypeClassBySystemAnyType := TryGetGenericTypeClassBySystemTypeClass(systemAnyType.TypeClass, systemAnyType.diSize, isObject, genericTypeClass => genericTypeClass);]]></ST>
      </Implementation>
    </Method>
    <Method Name="TryGetGenericTypeClassBySystemTypeClass" Id="{24ab732c-9169-4331-883f-5b4e80e0363a}">
      <Declaration><![CDATA[METHOD TryGetGenericTypeClassBySystemTypeClass : BOOL;
VAR_INPUT
	typeClass : __SYSTEM.TYPE_CLASS;	
	dataSize : DINT := 0; // needed for TYPE_ENUM and TYPE_SUBRANGE  
	isObject : BOOL := FALSE;
END_VAR
VAR_OUTPUT
	genericTypeClass : GENERIC_TYPE_CLASS;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[TryGetGenericTypeClassBySystemTypeClass := TRUE;

CASE typeClass OF
    __SYSTEM.TYPE_CLASS.TYPE_NONE:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_UNSPECIFIED;
    __SYSTEM.TYPE_CLASS.TYPE_BOOL:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_BOOL;
    __SYSTEM.TYPE_CLASS.TYPE_BYTE:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_BYTE;
    __SYSTEM.TYPE_CLASS.TYPE_WORD:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_WORD;
    __SYSTEM.TYPE_CLASS.TYPE_DWORD:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_DWORD;
    __SYSTEM.TYPE_CLASS.TYPE_LWORD:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_LWORD;
    __SYSTEM.TYPE_CLASS.TYPE_SINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_SINT;
    __SYSTEM.TYPE_CLASS.TYPE_INT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_INT;
    __SYSTEM.TYPE_CLASS.TYPE_DINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_DINT;
    __SYSTEM.TYPE_CLASS.TYPE_LINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_LINT;
    __SYSTEM.TYPE_CLASS.TYPE_USINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_USINT;
    __SYSTEM.TYPE_CLASS.TYPE_UINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_UINT;
    __SYSTEM.TYPE_CLASS.TYPE_UDINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_UDINT;
    __SYSTEM.TYPE_CLASS.TYPE_ULINT:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_ULINT;
    __SYSTEM.TYPE_CLASS.TYPE_REAL:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_REAL;
    __SYSTEM.TYPE_CLASS.TYPE_LREAL:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_LREAL;
    __SYSTEM.TYPE_CLASS.TYPE_STRING:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_STRING;
    __SYSTEM.TYPE_CLASS.TYPE_WSTRING:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_WSTRING;
    __SYSTEM.TYPE_CLASS.TYPE_TIME:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_TIME;
    __SYSTEM.TYPE_CLASS.TYPE_LTIME:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_LTIME;
    __SYSTEM.TYPE_CLASS.TYPE_DATE:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_DATE;
    __SYSTEM.TYPE_CLASS.TYPE_DATEANDTIME:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_DATE_AND_TIME;
    __SYSTEM.TYPE_CLASS.TYPE_TIMEOFDAY:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_TIME_OF_DAY;
    __SYSTEM.TYPE_CLASS.TYPE_POINTER:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_POINTER;
    __SYSTEM.TYPE_CLASS.TYPE_SUBRANGE, __SYSTEM.TYPE_CLASS.TYPE_ENUM:
		CASE dataSize OF
			1:
				genericTypeClass := GENERIC_TYPE_CLASS.TYPE_BYTE;
			2:
				genericTypeClass := GENERIC_TYPE_CLASS.TYPE_WORD;
			4:
				genericTypeClass := GENERIC_TYPE_CLASS.TYPE_DWORD;
			8:
				genericTypeClass := GENERIC_TYPE_CLASS.TYPE_LWORD;			
			ELSE
				TryGetGenericTypeClassBySystemTypeClass := FALSE;
		END_CASE
    __SYSTEM.TYPE_CLASS.TYPE_ARRAY:
		genericTypeClass := GENERIC_TYPE_CLASS.TYPE_ARRAY;
    __SYSTEM.TYPE_CLASS.TYPE_USERDEF:
		IF isObject THEN
			genericTypeClass := GENERIC_TYPE_CLASS.TYPE_OBJECT;
		ELSE
			genericTypeClass := GENERIC_TYPE_CLASS.TYPE_USER_DEFINED;
		END_IF

	ELSE
		TryGetGenericTypeClassBySystemTypeClass := FALSE;
END_CASE]]></ST>
      </Implementation>
    </Method>
  </POU>
</TcPlcObject>