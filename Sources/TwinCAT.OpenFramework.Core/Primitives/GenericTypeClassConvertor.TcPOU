<TcPlcObject Version="1.1.0.1">
  <POU Name="GenericTypeClassConvertor" Id="{3926d7cc-d133-4c1c-820a-65e2a022d573}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM GenericTypeClassConvertor
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{2bc98fce-998e-4726-842b-4d1bf26e3a6b}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{6394651d-dedc-4a8d-a7dc-57009b08f65a}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="GetGenericTypeClassBySystemAnyType" Id="{6fc44820-979c-4059-a13e-3c96b638aa44}">
      <Declaration><![CDATA[METHOD GetGenericTypeClassBySystemAnyType : GENERIC_TYPE_CLASS
VAR_INPUT
	systemAnyType : __SYSTEM.AnyType;	
	isObject : BOOL := FALSE;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[GetGenericTypeClassBySystemAnyType := GetGenericTypeClassBySystemTypeClass(systemAnyType.TypeClass, systemAnyType.diSize, isObject);]]></ST>
      </Implementation>
    </Method>
    <Method Name="GetGenericTypeClassBySystemTypeClass" Id="{74554218-289c-4bce-8cf6-fcb344d5e350}">
      <Declaration><![CDATA[METHOD GetGenericTypeClassBySystemTypeClass : GENERIC_TYPE_CLASS
VAR_INPUT
	typeClass : __SYSTEM.TYPE_CLASS;	
	dataSize : DINT := 0; // needed for TYPE_ENUM and TYPE_SUBRANGE
	isObject : BOOL := FALSE;  
END_VAR
VAR 
	invalidEnumTypeSizeException : NotSupportedException;
	typeClassNotSupportedException : ArgumentValueNotSupportedException;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE typeClass OF
    __SYSTEM.TYPE_CLASS.TYPE_NONE:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_UNSPECIFIED;
    __SYSTEM.TYPE_CLASS.TYPE_BOOL:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_BOOL;
    __SYSTEM.TYPE_CLASS.TYPE_BYTE:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_BYTE;
    __SYSTEM.TYPE_CLASS.TYPE_WORD:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_WORD;
    __SYSTEM.TYPE_CLASS.TYPE_DWORD:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_DWORD;
    __SYSTEM.TYPE_CLASS.TYPE_LWORD:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_LWORD;
    __SYSTEM.TYPE_CLASS.TYPE_SINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_SINT;
    __SYSTEM.TYPE_CLASS.TYPE_INT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_INT;
    __SYSTEM.TYPE_CLASS.TYPE_DINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_DINT;
    __SYSTEM.TYPE_CLASS.TYPE_LINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_LINT;
    __SYSTEM.TYPE_CLASS.TYPE_USINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_USINT;
    __SYSTEM.TYPE_CLASS.TYPE_UINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_UINT;
    __SYSTEM.TYPE_CLASS.TYPE_UDINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_UDINT;
    __SYSTEM.TYPE_CLASS.TYPE_ULINT:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_ULINT;
    __SYSTEM.TYPE_CLASS.TYPE_REAL:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_REAL;
    __SYSTEM.TYPE_CLASS.TYPE_LREAL:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_LREAL;
    __SYSTEM.TYPE_CLASS.TYPE_STRING:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_STRING;
    __SYSTEM.TYPE_CLASS.TYPE_WSTRING:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_WSTRING;
    __SYSTEM.TYPE_CLASS.TYPE_TIME:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_TIME;
    __SYSTEM.TYPE_CLASS.TYPE_LTIME:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_LTIME;
    __SYSTEM.TYPE_CLASS.TYPE_DATE:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_DATE;
    __SYSTEM.TYPE_CLASS.TYPE_DATEANDTIME:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_DATE_AND_TIME;
    __SYSTEM.TYPE_CLASS.TYPE_TIMEOFDAY:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_TIME_OF_DAY;
    __SYSTEM.TYPE_CLASS.TYPE_POINTER:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_POINTER;
    __SYSTEM.TYPE_CLASS.TYPE_SUBRANGE, __SYSTEM.TYPE_CLASS.TYPE_ENUM:
		CASE dataSize OF
			1:
				GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_BYTE;
			2:
				GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_WORD;
			4:
				GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_DWORD;
			8:
				GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_LWORD;			
			ELSE
				invalidEnumTypeSizeException.Throw(NamespaceName, ClassName, __POUNAME(), __POSITION(), WideStringHelper.ConcatStrings255("Enum has invalid data size ", TO_WSTRING(dataSize)));
		END_CASE
    __SYSTEM.TYPE_CLASS.TYPE_ARRAY:
		GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_ARRAY;
    __SYSTEM.TYPE_CLASS.TYPE_USERDEF:
		IF NOT isObject THEN
			GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_USER_DEFINED;
		ELSE
			GetGenericTypeClassBySystemTypeClass := GENERIC_TYPE_CLASS.TYPE_OBJECT;
		END_IF

	ELSE
		typeClassNotSupportedException.Throw(NamespaceName, ClassName, __POUNAME(), __POSITION(), 'typeClass', typeClass);
END_CASE]]></ST>
      </Implementation>
    </Method>
    <Property Name="NamespaceName" Id="{35b40298-c219-4a08-bf6c-45c25d4752ae}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY NamespaceName : STRING]]></Declaration>
      <Get Name="Get" Id="{667594df-2764-4d1e-a262-685580688ace}">
