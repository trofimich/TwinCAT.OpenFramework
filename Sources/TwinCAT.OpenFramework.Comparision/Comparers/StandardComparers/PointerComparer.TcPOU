      <Get Name="Get" Id="{f8221be0-963d-4b71-bc79-268f5d736232}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[NamespaceName := CurrentNamespace.Name;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="Size" Id="{0af2190f-35da-4e61-91d2-ccf31be2c3e2}">
