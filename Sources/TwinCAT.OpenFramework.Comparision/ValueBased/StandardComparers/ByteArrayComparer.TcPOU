    <Method Name="Compare" Id="{ee7448b3-b623-4697-a6c8-19f6c43d07cb}">
      <Declaration><![CDATA[{attribute "object_name" := "Compare"}
METHOD Compare : COMPARISION_RESULT
VAR_INPUT
	left : __SYSTEM.AnyType;
	right : __SYSTEM.AnyType;
END_VAR
VAR
	leftPointer : POINTER TO BYTE; 
	rightPointer : POINTER TO BYTE; 
	
	i : LINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF left.pValue = 0 AND right.pValue = 0 THEN
	Compare := COMPARISION_RESULT.FIRST_EQUAL_TO_SECOND;
	RETURN;
END_IF

IF left.pValue = 0 THEN
	Compare := COMPARISION_RESULT.FIRST_LESS_THAN_SECOND;
	RETURN;	
END_IF

IF right.pValue = 0 THEN
	Compare := COMPARISION_RESULT.FIRST_GREATER_THAN_SECOND;
	RETURN;	
END_IF

FOR i := 0 TO TO_LINT(MIN(left.diSize, right.diSize)) - 1 DO
	leftPointer := left.pValue + TO_LWORD(i);
	rightPointer := right.pValue + TO_LWORD(i);
	
	IF leftPointer^ < rightPointer^ THEN
		Compare := COMPARISION_RESULT.FIRST_LESS_THAN_SECOND;
		RETURN;
	ELSIF leftPointer^ > rightPointer^ THEN
		Compare := COMPARISION_RESULT.FIRST_GREATER_THAN_SECOND;
		RETURN;
	END_IF	
END_FOR

IF left.diSize < right.diSize THEN
	Compare := COMPARISION_RESULT.FIRST_LESS_THAN_SECOND;
ELSIF left.diSize > right.diSize THEN
	Compare := COMPARISION_RESULT.FIRST_GREATER_THAN_SECOND;
ELSE
	Compare := COMPARISION_RESULT.FIRST_EQUAL_TO_SECOND;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Property Name="Size" Id="{b1a1240f-8b51-48b9-84a3-3c39c9b5cd85}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY Size : ULINT]]></Declaration>
