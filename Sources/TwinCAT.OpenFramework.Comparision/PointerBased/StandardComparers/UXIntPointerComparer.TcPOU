  <POU Name="UXIntPointerComparer" Id="{46b094cf-0d83-43dd-8f55-8c90ddd9417a}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK UXIntPointerComparer EXTENDS Object IMPLEMENTS IPointerComparer
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{3d1e4338-d3e6-497c-97d6-0ac58f71bef5}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{db0bdfc1-7f8e-41fc-95ee-bc780c15b1fb}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
        </Implementation>
