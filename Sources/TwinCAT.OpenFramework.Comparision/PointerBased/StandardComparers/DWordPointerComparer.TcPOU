      <Get Name="Get" Id="{378fdb78-a43a-4070-b754-e79f7db1d2d8}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
