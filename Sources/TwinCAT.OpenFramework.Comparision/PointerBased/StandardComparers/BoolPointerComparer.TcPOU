  <POU Name="BoolPointerComparer" Id="{3cf83066-dc40-44af-9909-e962b0df2d66}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK BoolPointerComparer EXTENDS Object IMPLEMENTS IPointerComparer
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{ac607861-f9c4-4fc5-88b4-23e314146eb9}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{d275df2b-f9bd-4407-95e4-dc8355336a84}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
