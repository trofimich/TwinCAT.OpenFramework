  <POU Name="RealPointerComparer" Id="{5fc88d0b-c615-441a-b291-3952924265a7}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK RealPointerComparer EXTENDS Object IMPLEMENTS IPointerComparer
VAR
	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{e3872d0c-7331-4357-a9f0-7cfad33a6155}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY ClassName : STRING]]></Declaration>
      <Get Name="Get" Id="{1f991277-97ae-467b-9eb9-8253b2fe051b}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
