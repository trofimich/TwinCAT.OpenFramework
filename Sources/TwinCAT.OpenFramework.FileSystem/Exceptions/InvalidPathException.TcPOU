      <Get Name="Get" Id="{266b1451-a7f7-4677-9a17-074c37418502}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[ClassName := _ClassName;]]></ST>
        </Implementation>
