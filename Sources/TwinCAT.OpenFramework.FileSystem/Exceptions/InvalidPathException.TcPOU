	_ClassName : STRING := __POUNAME();
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="ClassName" Id="{6d3da3bd-a6e5-45c1-bda2-e3e02c19b687}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
