    <Method Name="Throw" Id="{d47f41f8-35c7-48fd-869f-e96f29dd4029}">
      <Declaration><![CDATA[METHOD Throw
VAR_INPUT
	sourceNamespaceName : STRING;
	sourceClassName : STRING;
	sourceMethodName : STRING;
	sourcePosition : STRING;
END_VAR
VAR_IN_OUT CONSTANT
	path : STRING(255);
END_VAR
VAR_INPUT
	errorMessage : WSTRING(255);
END_VAR
VAR
	stringBuilder : WideStringBuilder;
	message : ERROR_MESSAGE;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[stringBuilder
	.AppendString("Invalid path ")
	.AppendString255(STRING_TO_WSTRING('$22'))
	.AppendString255(STRING_TO_WSTRING(path))
	.AppendString255(STRING_TO_WSTRING('$22: '))
	.Append(errorMessage)
	.AssignToString(message);

InternalThrow(sourceNamespaceName, sourceClassName, sourceMethodName, sourcePosition, message, 0, 0);]]></ST>
      </Implementation>
    </Method>
