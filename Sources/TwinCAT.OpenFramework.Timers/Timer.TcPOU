PROPERTY Busy : BOOL
]]></Declaration>
      <Get Name="Get" Id="{e208484a-ee02-4797-bf54-88676e0118a7}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[Busy := _Timer.IN AND_THEN NOT _Timer.Q;]]></ST>
        </Implementation>
      </Get>
    </Property>
