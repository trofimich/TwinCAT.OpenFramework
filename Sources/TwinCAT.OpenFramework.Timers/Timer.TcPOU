    <Property Name="Busy" Id="{1181252f-edfb-4e0d-be8d-d3ce8b293d79}">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
{attribute 'OPC.UA.DA.Property' := '1'}
