VAR
	{attribute 'OPC.UA.DA' := '0'}
	_ClassName : STRING := __POUNAME();

	{attribute 'OPC.UA.DA' := '0'}
	_Timer : Tc2_Standard.TON;
	
	{attribute 'OPC.UA.DA' := '0'}
